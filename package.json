  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.1",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "request": "~2.88.0",
    "sequelize": "^5.21.2",
    "serve-favicon": "~2.4.5",
    "shrink-ray-current": "^4.1.2",
    "socket.io": "^2.4.0",
    "uuid": "^7.0.3"
